/******************************************************************************
 * File Name: main.c
 * Description: Application for using FIFO data structure
 * Author: Ehab Mohamed Abdelhamed
 ******************************************************************************/
#include "Platform_Types.h"
#include <stdio.h>
#include <string.h>
#include "fifo.h"

#define BUFFER_1_SIZE	7

ELEMENT_TYPE buffer1[BUFFER_1_SIZE];

/* Main Function */
int main(void){
	ELEMENT_TYPE data=0;
	FIFO_Buf_t UART_buf;
	FIFO_Buf_t SPI_buf;
	FIFO_init(&UART_buf, buffer1,  BUFFER_1_SIZE);
	uint32 i;
	uint8 *string="Ehab Abdelhamed";
	for(i=0;FIFO_pop(&UART_buf,&string[i]) == FIFO_NO_ERROR;i++);
	printf("UART_Fifo_Buffer get : ",data);
	while(FIFO_pop(&UART_buf,&data) == FIFO_NO_ERROR){
			printf(": %c\n",data);
	}

	return 0;
}
