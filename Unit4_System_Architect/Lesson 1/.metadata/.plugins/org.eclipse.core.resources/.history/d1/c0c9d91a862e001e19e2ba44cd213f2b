#ifndef LIFO_H_
#define LIFO_H_
#include "Platform_Types.h"
#include <stdlib.h>

/*******************************************************************************
 *                              Module Data Types                              *
 *******************************************************************************/
typedef struct{
	uint32 length;
	uint32 count;
	uint32* head;
	uint32* base;
}LIFO_Buf_t;

typedef enum{
	LIFO_NO_ERROR,
	LIFO_FULL,
	LIFO_NULL,
	LIFO_EMPTY
}LIFO_Stutes;



#endif /* LIFO_H_ */
